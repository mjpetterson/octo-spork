;; Octo-Spork, a really simple palindrome detector
(defun yes () 
       (format t "Yes"))
(defun no ()
       (format t "No"))
(defun string-to-list (s)
       (coerce s 'list))
(defun isPalindrome (word)
       (equal word (reverse word)))
(defun Palindrome (word)
       (if (isPalindrome word)
       	   (yes)
	   (no)))
(Palindrome (string-to-list (read-line)))